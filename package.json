{
  "name": "Tag",
  "version": "1.0.0",
  "description": "A text annotation engine",
  "main": "main.js",
  "scripts": {
    "start": "electron ."
  },
  "build": {
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createStartMenuShortcut": true,
      "createDesktopShortcut": true
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^7.2.4"
  },
  "dependencies": {
    "jquery": "^3.4.1",
    "python-shell": "^1.0.8"
  }
}
